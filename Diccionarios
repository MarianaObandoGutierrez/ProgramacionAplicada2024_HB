      "cell_type": "code",
      "source": [
        "Grandmom ={\n",
        "        \"Name\" : \"Sofia\",\n",
        "        \"Age\" : 62\n",
        "    }\n",
        "Grand_dad={\n",
        "        \"Name\" : \"Carlos\",\n",
        "        \"Age\" : 63\n",
        "    }\n",
        "Mom={\n",
        "        \"Name\": \"Marlen\",\n",
        "        \"Age\":42\n",
        "    }\n",
        "Sister={\n",
        "        \"Name\":\"Gabriela\",\n",
        "        \"Age\":15\n",
        "    }\n",
        "Myfamily1={\n",
        "    \"Grandmom\" : Grandmom,\n",
        "    \"Grand_dad\" : Grand_dad,\n",
        "    \"Mom\" : Mom,\n",
        "    \"Sister\" : Sister\n",
        "}\n",
        "print(Myfamily1)\n",
        ""
